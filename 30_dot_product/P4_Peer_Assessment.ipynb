   "source": [
    "import numpy as np\n",
    "from numpy.linalg import norm\n",
    "\n",
    "\n",
    "def normalize(v):\n",
    "  k = 1 / norm(v)\n",
    "  return np.dot(k, v)\n",
    "\n",
    "\n",
    "v = np.array([-1,-2,2])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
